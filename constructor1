#include <iostream>
using namespace std;

class myClass {
    public:
        // myClass() is a constructor a special member function 
        // with the same name as the class myClass but with () function brackets
        myClass() {    
            cout <<"Hey";
        }
        void setName(string x) {
            name = x;
        }
        string getName() {
            return name;
        }
    private:
        string name;
};

int main() {
    myClass myObj;

    return 0;
}
